{
  "expo": {
    "name": "Playstore",
    "slug": "playstore",
    "owner": "olamrabt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/42-logo.png",
    "scheme": "playstore",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.olamrabt.playstore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/42-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.olamrabt.playstore"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/42-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/42-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/42-logo.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification-sound.wav"
          ]
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/42-logo.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification",
      "iosDisplayInForeground": true
    },
    "extra": {
      "apiUrl": "https://europe-west1-playstore-e4a65.cloudfunctions.net/api",
      "redirectUrl": "https://europe-west1-playstore-e4a65.cloudfunctions.net/api",
      "router": {},
      "eas": {
        "projectId": "playstore-app"
      }
    }
  }
}
